{
  "name": "@contextcompany/api",
  "version": "1.0.1",
  "description": "The Context Company core API utilities",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "pnpm build --watch"
  },
  "sideEffects": false,
  "keywords": [
    "contextcompany",
    "observability",
    "tracing",
    "feedback"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/The-Context-Company/observatory",
    "directory": "packages/ts/api"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@types/node": "^20",
    "prettier": "^3.8.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  }
}
